<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />

	<title> Mercado Liebre Argentina</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="./css/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="./css/app.css">
</head>

<body>
	<!-- Header -->
	<header class="main-header">
            <% if (userLogueado){%>
              <%- include('./partials/headerlogueado') %>
            <% } else { %>
              <%- include('./partials/header')
            <%}%>
	</header>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Ingresar</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
                <i>
                    <form action="login.ejs" method="GET" >

                        <div> <input type="email" name="email" placeholder="Escriba su usuario"></div>

                        <div> <input type="password" name="password" placeholder="Escriba su contraseña"></div>

                        <div> <input type="checkbox" name="recordar" placeholder="Recordarme en esta página"></div>


                    </form>
                </i>

			</h2>


	<!-- footer -->
	<footer class="main-footer">
        <%- includes('./partials/footer.ejs')%>
	</footer>
	<!-- /footer -->
	<script src="../css/app.css"></script>
</body>

</html>